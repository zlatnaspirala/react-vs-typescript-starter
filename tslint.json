{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "object-literal-sort-keys": [false, "ignore-case", "match-declaration-order"],
        "no-console": false,
        "no-namespace": false,
        "interface-name": [true, "never-prefix"],
        "no-var-requires": false,
        "interface-over-type-literal": false
    },
    "rulesDirectory": []
}